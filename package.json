{
  "name": "tdp",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "overrides": {
    "cross-spawn": "^7.0.5",
    "brace-expansion": "^2.0.1",
    "react-is": "^19.0.0"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "test:cleanup": "pkill -9 -f 'node.*vitest' || true",
    "pretest": "npm run test:cleanup",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.mjs",
    "test:integration:watch": "vitest --config vitest.integration.config.mjs",
    "test:integration:coverage": "vitest run --config vitest.integration.config.mjs --coverage",
    "test:all": "npm run test:run && npm run test:integration && npm run test:e2e:critical",
    "test:e2e": "playwright test",
    "test:e2e:critical": "playwright test --config=playwright.critical.config.ts",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:e2e:report:critical": "playwright show-report playwright-report-critical",
    "test:e2e:i18n": "bash scripts/run-e2e-tests.sh i18n",
    "test:e2e:likes": "bash scripts/run-e2e-tests.sh likes",
    "test:e2e:auth": "bash scripts/run-e2e-tests.sh auth",
    "test:e2e:sitemap": "bash scripts/run-e2e-tests.sh sitemap",
    "test:e2e:all": "bash scripts/run-e2e-tests.sh all",
    "test:e2e:seed": "tsx e2e/utils/seed-test-data.ts",
    "test:e2e:cleanup": "tsx e2e/utils/seed-test-data.ts cleanup",
    "test:e2e:admin-tests": "playwright test e2e/content-operations.spec.ts --project=chromium",
    "test:check-coverage": "bash scripts/check-coverage.sh",
    "test:coverage-summary": "node scripts/coverage-summary.js",
    "test:features": "vitest run src/config/__tests__",
    "test:credentials": "vitest run src/app/admin/credentials src/components/admin/credential src/lib/credentials",
    "test:admin": "vitest run src/app/admin src/components/admin",
    "test:admin:smoke": "playwright test e2e/content-operations.spec.ts --project=chromium --reporter=line",
    "db:dedupe:gallery": "tsx scripts/dedupe-gallery.ts",
    "db:generate-thumbs": "tsx scripts/generate-thumbnails.ts",
    "generate-key": "tsx scripts/generate-encryption-key.ts",
    "migrate-encrypt-credentials": "tsx scripts/migrate-encrypt-credentials.ts",
    "features:list": "tsx scripts/list-features.ts",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@aws-sdk/client-s3": "^3.899.0",
    "@aws-sdk/lib-storage": "^3.900.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/mime-types": "^3.0.1",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "cmdk": "^1.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "exifr": "^7.1.3",
    "framer-motion": "^12.23.24",
    "jszip": "^3.10.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.545.0",
    "mime-types": "^3.0.1",
    "next": "15.5.4",
    "next-auth": "^4.24.11",
    "node-cron": "^4.2.1",
    "node-geocoder": "^4.4.1",
    "nodemailer": "^6.10.1",
    "pinyin-pro": "^3.18.4",
    "preact": "10.11.3",
    "prisma": "^6.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-leaflet": "^5.0.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.3.0",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/adm-zip": "^0.5.7",
    "@types/crypto-js": "^4.2.2",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^22.18.8",
    "@types/node-geocoder": "^4.2.6",
    "@types/nodemailer": "^6.4.20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/sharp": "^0.32.0",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "adm-zip": "^0.5.16",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jose": "^6.1.0",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "typescript-eslint": "^8.44.1",
    "vitest": "^3.2.4",
    "xml2js": "^0.6.2"
  }
}
